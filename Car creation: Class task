//make body
string $body[] = `polyCube -w 1 -h 1 -d 1 -sx 1 -sy 1 -sz 1 -ax 0 1 0 -cuv 4 -ch 1`;
select $body;
scale 4 3 6;
move 0 2 0;

//Wheels
polyCylinder;
select -r pCylinder1 ;
rotate 90 90 0;
scale 0.8 1 0.8;
duplicate -rr;
duplicate -rr;
duplicate -rr;
select -r pCylinder2 ;
select -r pCylinder3 ;
select -r pCylinder4 ;
